{
    "name": "mirko/testera",
    "type": "library",
    "description": "test",
    "keywords": [
        "mirko",
        "testera"
    ],
    "homepage": "https://github.com/mirko/testera",
    "license": "MIT",
    "authors": [
        {
            "name": "mirko",
            "email": "bukela@gmail.com",
            "homepage": "https://github.com/bukela",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.6|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "mirko\\testera\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "mirko\\testera\\": "tests"
        }
    },
    "extra": {
            "laravel": {
                "providers": [
                    "Mirko\\Testera\\TesteraServiceProvider"
                ]
            }
    },
    "config": {
        "sort-packages": true
    }
}
